cesContent":["<template>\r\n  <div>  <!-- 模板要求：根元素-->\r\n  <!-- 昵称<input type=\"text\" placeholder=\"请填写昵称\"><br>\r\n  手机号<input type=\"text\" placeholder=\"请绑定手机号码\"><br>\r\n  性别<input type=\"radio\" name=\"sex\" value=\"man\">男\r\n       <input type=\"radio\" name=\"sex\" value=\"woman\">女<br>\r\n  生日<input type=\"text\" placeholder=\"请选择出生日期\">\r\n      <p>注：生日保存之后不允许修改，请正确填写</p>\r\n      <div >\r\n          <p>请完善个人信息，以便我们为你提供更好的会员服务</p>\r\n      </div>\r\n      <div class=\"Bottom\">\r\n            <div>保存</div>\r\n            <div>退出登录</div>\r\n      </div> -->\r\n      <router-link to=\"/Me\"><img src=\"../../img/zuojiantou.png\"  class=\"gtt\"></router-link>\r\n     <mt-field label=\"昵称\" placeholder=\"请填写昵称\" v-model=\"uname\" :attr=\"{autofocus:true}\"></mt-field>\r\n     <mt-field label=\"手机号\" placeholder=\"请绑定手机号码\"  v-model=\"phone\"></mt-field>\r\n     <div class=\"sex\">\r\n       <p class=\"sex2\">性别</p>\r\n    <div class=\"radio1\"  @click=\"man\">\r\n         <img src=\"../../img/radio.png\" class=\"radio-img\"  v-show=\"alert\">\r\n       </div>\r\n        <p class=\"sex3\"  @click=\"man\">男</p>\r\n       <div class=\"radio2\"  @click=\"woman\">\r\n          <img src=\"../../img/radio.png\" class=\"radio-img\" v-show=\"alert2\">\r\n           </div>\r\n      <p class=\"sex4\" @click=\"woman\">女</p>\r\n     </div>\r\n     <mt-field label=\"生日\" placeholder=\"请选择出生日期\" v-model=\"uage\"></mt-field>\r\n     <mt-button @click=\"login\">注册</mt-button>\r\n    \r\n  </div>   \r\n</template>\r\n<script>\r\n export default{\r\n     data() {\r\n  return {\r\n    uname:\"\",\r\n    phone:\"\",\r\n    uage:\"\",\r\n    alert:true,\r\n    alert2:false\r\n  }\r\n},\r\n     \r\n    methods:{\r\n        man(){\r\n           if(this.alert=true){\r\n             this.alert2=false;\r\n           }\r\n        },\r\n        woman(){\r\n          if(this.alert2=true){\r\n            this.alert=false;\r\n          }\r\n        },\r\n        //添加登录函数\r\n        login(){\r\n            //对表单中的每个元素进行验证\r\n            //验证：去掉用户无效操作\r\n            //1.创建用户名和密码正则表达式\r\n            var ureg=/^[a-z0-9]{3,12}$/i;\r\n            //2.创建年龄正则表达式\r\n            var areg=/^[0-9]{1,2}$/;\r\n            //3.创建手机号正则表达式\r\n            var phone=/^1[3456789]\\d{9}$/;\r\n            //3.获取用户输入用户名\r\n            var u=this.uname;\r\n            console.log(u)\r\n            //4.获取用户输入的手机号\r\n            var p=this.phone;\r\n            console.log(p)\r\n            //5.获取用户输入的年龄\r\n            var age=this.uage;\r\n            //6.通过正则表达式来验证用户名是否正确\r\n            if(ureg.test(u)==false){\r\n                 //7.如果验证不正确显示确认消息框\r\n                this.$messagebox(\"消息\",\"昵称格式不正确\")\r\n           \r\n            //8.停在函数执行\r\n            return;\r\n           }\r\n             //10.通过正则表达式验证年龄是否合法\r\n           if(areg.test(age)==false){\r\n           //11.如果验证没通过显示确认信息\r\n           this.$messagebox(\"消息\",\"年龄格式不正确\")\r\n           //12.停在函数执行                               \r\n           return;\r\n             }\r\n            //9.通过正则表达式验证生日是否合法\r\n           if(phone.test(p)==false){\r\n           //10.如果验证没通过显示确认信息\r\n           this.$messagebox(\"消息\",\"手机号格式不正确\")\r\n           //11.停在函数执行 \r\n           return;\r\n           }\r\n          \r\n         },\r\n         \r\n   }\r\n }\r\n</script>   \r\n \r\n<style scope>\r\n    .Bottom{display:flex;\r\n    justify-content: space-between;}\r\n   .sex{\r\n     display: flex;\r\n     border-top:1px solid #999999;\r\n     padding-right:10px;\r\n\r\n   } \r\n   .radio1,.radio2{\r\n     width:20px;\r\n     height:20px;\r\n     border-radius: 50%;\r\n     border:1px solid red;\r\n     margin-left:67px;\r\n     margin-top:7px;\r\n   }\r\n   .radio-img{\r\n     width:20px;height:20px;\r\n     \r\n   }\r\n   .sex2{\r\n     margin-left:10px;\r\n   }\r\n    .sex3{\r\n     margin-left:5px;\r\n     margin-right:-25px;\r\n   }\r\n    .sex4{\r\n     margin-left:5px;\r\n   }\r\n</style>"]}]}